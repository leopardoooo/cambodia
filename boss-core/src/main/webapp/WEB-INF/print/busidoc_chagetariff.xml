<?xml version="1.0" encoding="GBK"?>
<div class="Cl">
 
 	<tpl if="params.user_type == '模拟电视'">
		<div class="print_item">
			<div class="row">
				<div class="title_1">业务{point}</div>
				<div class="title_2">资费变更</div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;"> <span>业务类型：模拟电视</span></div>
				<div class="field" style="width: 40%;"> <span>终端类型：主终端</span></div>
			</div>
			 <div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>产品名称：{[values.params.prod_name]}</span></div>
				<div class="field" style="width: 40%;" > <span>旧计费方式：{[values.params.old_tariff_name]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>新计费方式：{[values.params.new_tariff_name]}</span></div>
				<div class="field" style="width: 40%;" > <span>生效日期：{[values.params.eff_date]}</span></div>
			</div>
		</div>
	</tpl>

	<tpl if="params.user_type == '数字电视'">
		<div class="print_item">
			<div class="row">
				<div class="title_1">业务{point}</div>
				<div class="title_2">资费变更<!-- 【流水号：{[values.item.done_code]}】 --></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;"> <span>业务类型：数字电视</span></div>
				<div class="field" style="width: 40%;"> <span>终端类型：{[values.params.terminal_type]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>机顶盒号：{[values.params.stb_id]}</span></div>
				<div class="field" style="width: 40%;" > <span>智能卡号：{[values.params.card_id]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>产品名称：{[values.params.prod_name]}</span></div>
				<div class="field" style="width: 40%;" > <span>旧计费方式：{[values.params.old_tariff_name]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>新计费方式：{[values.params.new_tariff_name]}</span></div>
				<div class="field" style="width: 40%;" > <span>生效日期：{[values.params.eff_date]}</span></div>
			</div>
		</div>
	</tpl>
 <tpl if="params.user_type == '宽带'">
	<div class="print_item">
		<div class="row">
			<div class="title_1">业务{point}</div>
			<div class="title_2">资费变更</div>
		</div>
		<div class="row">
			<div class="title_1">&nbsp;</div>
			<div class="field"> <span>业务类型：互联网</span></div>
		</div>
		<div class="row">
			<div class="title_1">&nbsp;</div>
			<div class="field" style="width: 50%;"> <span>上网帐号：{[values.params.login_name]}</span></div>
		</div>
		<div class="row">
			<div class="title_1">&nbsp;</div>
 			<div class="field" style="width: 50%;" > <span>旧套餐：{[values.params.old_tariff_name]}</span></div>
			<div class="field" style="width: 40%;" > <span>新套餐：{[values.params.new_tariff_name]}</span></div>
			
		</div>
		<div class="row">
			<div class="title_1">&nbsp;</div>
 			<div class="field" style="width: 50%;" > <span>生效日期：{[values.params.eff_date]}</span></div>
		</div>
	</div>
</tpl>
</div>