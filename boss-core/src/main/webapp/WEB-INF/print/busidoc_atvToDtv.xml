<?xml version="1.0" encoding="GBK"?>
<div class="Cl">
 
<tpl if="params.user_type == '数字电视'">
	<div class="print_item">
			<div class="row">
				<div class="title_1">业务{point}</div>
				<div class="title_2">数字电视平移</div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field"> <span>业务类型：数字电视</span></div>
			</div>

			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;" > <span>机顶盒号：{[values.params.stb_id]}</span></div>
				<div class="field" style="width: 40%;"> <span>智能卡号：{[values.params.card_id]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;"> <span>终端类型：{[values.params.terminal_type]}</span></div>
				<div class="field" style="width: 40%;"> <span>购买方式：{[values.params.stb_buy_mode]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field" style="width: 50%;"> <span>产品名称：{[values.params.prod_name]}</span></div>
				<div class="field" style="width: 40%;"> <span>计费方式：{[values.params.tariff_name]}</span></div>
			</div>
			<div class="row">
				<div class="title_1">&nbsp;</div>
				<div class="field">&nbsp;</div>
				<div class="field">&nbsp;</div>
			</div>
		
		
	</div>
</tpl>
 
</div>